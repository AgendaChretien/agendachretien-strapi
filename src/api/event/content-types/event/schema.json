{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::city.city"
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "picture": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date"
    },
    "startTime": {
      "type": "time"
    },
    "endTime": {
      "type": "time"
    },
    "url": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "extraPictures": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["images", "files", "videos", "audios"]
    }
  }
}
